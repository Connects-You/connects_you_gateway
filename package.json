{
	"name": "connects-you-gateway",
	"packageManager": "yarn@3.2.3",
	"scripts": {
		"build": "tsc -p tsconfig.release.json && mkdir -p ./build/src/schemas/graphql && cp -rf ./src/schemas/graphql/* ./build/src/schemas/graphql/",
		"lint": "eslint ./src --ext .ts,.js --fix",
		"postinstall": "husky install && tsc -p tsconfig.release.json",
		"prestart": "tsc -p tsconfig.release.json",
		"start": "node ./build/src/index.js",
		"dev": "nodemon ./src/index.ts -e ts,json",
		"startLocalServer": "ngrok http 3000",
		"kill": "sudo kill -9 `sudo lsof -t -i:3000`"
	},
	"author": "Adarsh Mishra",
	"license": "ISC",
	"dependencies": {
		"@adarsh-mishra/connects_you_services": "https://github.com/Connects-You/connects_you_services",
		"@adarsh-mishra/node-utils": "https://github.com/AdarshMishraji/node-utils",
		"@graphql-tools/load-files": "^6.6.1",
		"@grpc/grpc-js": "^1.7.1",
		"@grpc/proto-loader": "^0.7.3",
		"apollo-server-express": "^3.10.3",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.0.2",
		"express-http-context": "^1.2.4",
		"graphql": "^16.6.0",
		"graphql-subscriptions": "^2.0.0",
		"graphql-tools": "^8.3.6",
		"graphql-ws": "^5.11.2",
		"tsutils": "^3.21.0",
		"ws": "^8.9.0"
	},
	"devDependencies": {
		"@types/compression": "^1.7.2",
		"@types/express": "^4.17.14",
		"@types/express-rate-limit": "^6.0.0",
		"@types/jsonwebtoken": "^8.5.9",
		"@types/node": "^18.7.19",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-sonarjs": "^0.15.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3"
	},
	"version": "0.0.1"
}
